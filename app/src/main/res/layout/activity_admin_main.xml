    <TextView
        android:id="@+id/whaddpage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Warehouse"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/inp_wh"
        android:layout_width="357dp"
        android:layout_height="46dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Enter Warehouse Name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/listviewwh"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/whaddpage"
        app:layout_constraintTop_toBottomOf="@+id/whaddpage" />

    <EditText
        android:id="@+id/inp_whcap"
        android:layout_width="358dp"
        android:layout_height="45dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:hint="Enter capacity"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/inp_wh"
        app:layout_constraintStart_toStartOf="@+id/inp_wh"
        app:layout_constraintTop_toBottomOf="@+id/inp_wh" />

    <TextView
        android:id="@+id/listviewwh"
        android:layout_width="82dp"
        android:layout_height="28dp"
        android:layout_marginLeft="152dp"
        android:layout_marginStart="152dp"
        android:text="List View"
        android:textColor="#2196F3"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/whaddpage"
        app:layout_constraintStart_toEndOf="@+id/whaddpage" />

    <Button
        android:id="@+id/savewh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/deletewh"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/deletewh" />

    <Button
        android:id="@+id/deletewh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:layout_marginRight="84dp"
        android:layout_marginTop="48dp"
        android:text="Delete"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inp_whcap" />

    <Button
        android:id="@+id/addstops"
        android:layout_width="159dp"
        android:layout_height="52dp"
        android:layout_marginTop="16dp"
        android:text="Add stops"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/savewh" />

